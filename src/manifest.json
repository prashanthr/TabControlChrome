{
  "manifest_version": 2,

  "name": "Tab Control",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "master.html",
    "default_title": "Tab Control"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}